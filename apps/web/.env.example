# AWS ECS Fargate Configuration
AWS_REGION="eu-west-2"
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""
ECS_CLUSTER=""
ECS_TASK_DEFINITION=""
ECS_CONTAINER_NAME=""
ECS_SUBNETS=""
ECS_SECURITY_GROUPS=""
ECS_LOG_GROUP=""

# Resend
RESEND_API_KEY=""

# Vercel Blob
BLOB_READ_WRITE_TOKEN=""
